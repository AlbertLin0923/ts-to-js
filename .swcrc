{
  "env": {
    "targets":"> 0.25%, not dead",
    "mode": "usage",
    "coreJs": 3
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": false
    },
    "target": "es2016",
    "loose": true,
    "externalHelpers": true
  },
  "module": {
    "type": "es6",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
    "ignoreDynamic": false
  },
  "sourceMaps": true
}
